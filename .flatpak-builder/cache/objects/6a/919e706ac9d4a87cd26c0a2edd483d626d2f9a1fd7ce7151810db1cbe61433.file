[Application]
name=io.github.mfat.portsinfo
runtime=org.gnome.Platform/x86_64/46
sdk=org.gnome.Sdk/x86_64/46
command=ports-info

[Context]
shared=network;ipc;
sockets=x11;wayland;fallback-x11;

[Extension io.github.mfat.portsinfo.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=io.github.mfat.portsinfo.Debug;